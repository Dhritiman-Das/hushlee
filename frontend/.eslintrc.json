{
  "extends": ["next/core-web-vitals", "airbnb", "airbnb-typescript"],
  "parserOptions": {
    "project": "./frontend/tsconfig.json"
  },
  "rules": {
    "linebreak-style": 0,
    "import/extensions": 0,
    "react/jsx-props-no-spreading": "off",
    "no-param-reassign": [
      "error",
      { "props": true, "ignorePropertyModificationsFor": ["state"] }
    ]
  }
}
